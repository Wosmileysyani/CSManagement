            @Html.ValidationMessageFor(model => model.CE_ATNO, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.DIS_DateOUT, htmlAttributes: new { @class = "control-label col-md-2" })
