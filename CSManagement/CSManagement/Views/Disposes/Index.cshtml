@model IEnumerable<CSManagement.Models.Dispose>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    int i = 1;
}

<div class="box">
    <div class="box-header">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <h1 class="box-title">ข้อมูลการจำหน่ายครุภัณฑ์</h1>
                <div style="float:right">
                    <a class="btn btn-success" href="@Url.Action("Create", "Disposes")"><i class="fa fa-plus"></i> เพิ่มข้อมูลการจำหน่ายครุภัณฑ์</a>
                </div>
            </div>
        </div>
    </div>
    <div class="box-body">
        <table id="example1" class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th style="text-align:center">ที่</th>
                    <th style="text-align:center">@Html.DisplayNameFor(model => model.CE_ATNO)</th>
                    <th style="text-align:center">@Html.DisplayNameFor(model => model.DIS_DateOUT)</th>
                    <th style="text-align:center">@Html.DisplayNameFor(model => model.DIS_DateAPP)</th>
                    <th style="text-align:center">@Html.DisplayNameFor(model => model.DIS_Status)</th>
                    @*<th></th>*@
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td style="text-align:center">@(i++)</td>
                        <td style="text-align:center">@Html.DisplayFor(modelItem => item.CE_ATNO)</td>
                        <td>@Html.DisplayFor(modelItem => item.DIS_DateOUT)</td>
                        <td>@Html.DisplayFor(modelItem => item.DIS_DateAPP)</td>
                        <td style="text-align:center">@Html.DisplayFor(modelItem => item.DIS_Status)</td>
                        @*<td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.DIS_ATNO }) |
                            @Html.ActionLink("Details", "Details", new { id = item.DIS_ATNO }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.DIS_ATNO })
                        </td>*@
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
