@model CSManagement.Models.Generation

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
    <h4 class="modal-title" style="letter-spacing: 0px; font-size: 20px">รายละเอียดหลักสูตร</h4>
</div>
<div class="modal-body">
    <table class="table" style="width: 100%">
        <tbody>

            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Short_Course.SC_NameTH)</th>
                <td>@Html.DisplayFor(modelItem => Model.Short_Course.SC_NameTH)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Name)</th>
                <td style="text-align: center">@Html.DisplayFor(modelItem => Model.Gen_Name)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Member)</th>
                <td style="text-align: center">@Html.DisplayFor(modelItem => Model.Gen_Member)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Year)</th>
                <td style="text-align: center">@Html.DisplayFor(modelItem => Model.Gen_Year)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Date)</th>
                <td style="text-align: center">@Html.DisplayFor(modelItem => Model.Gen_Date)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Location)</th>
                <td style="text-align: center">@Html.DisplayFor(modelItem => Model.Gen_Location)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Status1.Gen_Name)</th>
                <td style="text-align: center">@Html.DisplayFor(modelItem => Model.Gen_Status1.Gen_Name)</td>
            </tr>
            <tr>
                <th style="text-align: center">@Html.DisplayNameFor(model => model.Gen_Fee)</th>
                <td style="text-align: right"><strong>@Html.DisplayFor(modelItem => Model.Gen_Fee)</strong></td>
            </tr>
        </tbody>
    </table>

</div>
<div class="modal-footer">
    <a class="btn btn-default" href="@Url.Action("Registers", "Generations", new {id = Model.Gen_NO})"><i class="fa fa-edit"></i>สมัครคอร์ส</a>

    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
</div>
