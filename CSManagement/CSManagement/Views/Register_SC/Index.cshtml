@model IEnumerable<CSManagement.Models.Register_SC>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.REG_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REG_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REG_Tel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REG_Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.REG_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.REG_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.REG_Tel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.REG_Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.REG_IDCard }) |
            @Html.ActionLink("Details", "Details", new { id=item.REG_IDCard }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.REG_IDCard })
        </td>
    </tr>
}

</table>
